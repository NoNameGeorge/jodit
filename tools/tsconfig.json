{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 16",

	"compilerOptions": {
		"lib": ["es2021"],
		"module": "Node16",

		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,

		"target": "es5",
		"moduleResolution": "NodeNext",
		"allowSyntheticDefaultImports": true
	}
}
